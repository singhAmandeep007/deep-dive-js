{
  "name": "deep-dive-js",
  "version": "1.0.0",
  "description": "JavaScript and TypeScript examples",
  "scripts": {
    "start": "parcel index.html --open",
    "build": "parcel build index.html",
    "ts:check": "tsc --noEmit"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "parcel": "^2.11.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "typescript",
    "javascript"
  ],
  "alias": {
    "script/*": "./src/scripts/*"
  },
  "@parcel/transformer-css": {
    "cssModules": {
      "pattern": "[name]-[hash]-[local]"
    }
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "devDependencies": {
    "@types/css-modules": "^1.0.5"
  }
}
