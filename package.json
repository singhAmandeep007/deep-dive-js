{
  "name": "deep-dive-js",
  "version": "1.0.0",
  "description": "JavaScript and TypeScript examples",
  "scripts": {
    "start": "parcel index.html --open --no-cache",
    "build": "rm -rf dist .parcel-cache && parcel build index.html",
    "ts:check": "tsc --noEmit"
  },
  "dependencies": {
    "@codesandbox/sandpack-react": "^2.11.3",
    "clsx": "^2.1.0",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "parcel": "^2.11.0",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "typescript",
    "javascript"
  ],
  "alias": {
    "script/*": "./src/scripts/*"
  },
  "@parcel/transformer-css": {
    "cssModules": {
      "pattern": "[name]-[hash]-[local]"
    }
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "devDependencies": {
    "@types/css-modules": "^1.0.5",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "process": "^0.11.10"
  }
}
